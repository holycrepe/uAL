                            <Setter TargetName="DockResizer" Property="Height" Value="6"/>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0 0 0 6"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

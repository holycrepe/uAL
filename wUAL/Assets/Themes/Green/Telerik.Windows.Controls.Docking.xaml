                <Setter TargetName="PART_BottomIndicator" Property="Background" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="PART_BottomIndicator" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="BottomPlacementIndicator" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="BottomHighlightVisual" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="DockPosition" Value="Center">
                <Setter TargetName="PART_CenterIndicator" Property="Background" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="PART_CenterIndicator" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="CenterHighlightVisual" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsLeftIndicatorVisible" Value="False">
                <Setter TargetName="PART_LeftIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsTopIndicatorVisible" Value="False">
                <Setter TargetName="PART_TopIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsRightIndicatorVisible" Value="False">
                <Setter TargetName="PART_RightIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsBottomIndicatorVisible" Value="False">
                <Setter TargetName="PART_BottomIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsCenterIndicatorVisible" Value="False">
                <Setter TargetName="PART_CenterIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="CompassStyle" TargetType="dock:Compass">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="Template" Value="{StaticResource CompassControlTemplate}"/>
    </Style>
    <ControlTemplate x:Key="RootCompassControlTemplate" TargetType="dock:Compass">
        <Grid x:Name="VisualRoot">
            <Border x:Name="PART_LeftIndicator"
                    Grid.Row="1"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Width="30"
                    SnapsToDevicePixels="True"
                    Height="30"
                    Background="{telerik:GreenResource ResourceKey=PrimaryBrush}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="1"
                    Margin="5 3 0 0"
                    CornerRadius="{telerik:GreenResource ResourceKey=CornerRadius}">
                <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                    <Border x:Name="LeftHighlightVisual" BorderThickness="1 4 1 1" BorderBrush="{telerik:GreenResource ResourceKey=AccentLowBrush}" Height="24" Width="12" Margin="2 0 0 0"/>
                    <Path x:Name="LeftPlacementIndicator"
                            Fill="{telerik:GreenResource ResourceKey=StrongBrush}"
                            Data="{StaticResource ArrowLeft}"
                            VerticalAlignment="Center"
                            Margin="8 0 0 0"
                            Stretch="None"/>
                </StackPanel>
            </Border>
            <Border x:Name="PART_RightIndicator"
                    SnapsToDevicePixels="True"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Grid.Row="1"
                    Grid.Column="2"
                    Margin="0 3 5 0"
                    Width="30"
                    Height="30"
                    Background="{telerik:GreenResource ResourceKey=PrimaryBrush}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="1"
                    CornerRadius="{telerik:GreenResource ResourceKey=CornerRadius}">
                <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                    <Path x:Name="RightPlacementIndicator"
                            Fill="{telerik:GreenResource ResourceKey=StrongBrush}"
                            Data="{StaticResource ArrowRight}"
                            VerticalAlignment="Center"
                            Margin="2 0 8 0"/>
                    <Border x:Name="RightHighlightVisual"
                            BorderThickness="1 4 1 1"
                            BorderBrush="{telerik:GreenResource ResourceKey=AccentLowBrush}"
                            Height="24"
                            Width="12"
                            Margin="0 1 1 1"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"/>
                </StackPanel>
            </Border>
            <Border x:Name="PART_BottomIndicator"
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Center"
                    Margin="0 0 0 5"
                    SnapsToDevicePixels="True"
                    Grid.Row="2"
                    Grid.Column="1"
                    Width="30"
                    Height="30"
                    Background="{telerik:GreenResource ResourceKey=PrimaryBrush}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="1"
                    CornerRadius="{telerik:GreenResource ResourceKey=CornerRadius}">
                <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                    <Path x:Name="BottomPlacementIndicator"
                            Fill="{telerik:GreenResource ResourceKey=StrongBrush}"
                            Data="{StaticResource ArrowDown}"
                            HorizontalAlignment="Center"
                            Margin="0 2 0 8"/>
                    <Border x:Name="BottomHighlightVisual"
                            BorderThickness="1 4 1 1"
                            BorderBrush="{telerik:GreenResource ResourceKey=AccentLowBrush}"
                            Height="12"
                            Width="24"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Margin="1 0 1 0"/>
                </StackPanel>
            </Border>
            <Border x:Name="PART_TopIndicator"
                    Grid.Column="1"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Center"
                    Width="30"
                    Height="30"
                    Margin="0 5 0 0"
                    SnapsToDevicePixels="True"
                    Background="{telerik:GreenResource ResourceKey=PrimaryBrush}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="1"
                    CornerRadius="{telerik:GreenResource ResourceKey=CornerRadius}">
                <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                    <Border x:Name="TopHighlightVisual" BorderThickness="1 4 1 1" BorderBrush="{telerik:GreenResource ResourceKey=AccentLowBrush}" Height="12" Width="24" Margin="1 2 1 8"/>
                    <Path x:Name="TopPlacementIndicator"
                            Fill="{telerik:GreenResource ResourceKey=StrongBrush}"
                            Data="{StaticResource ArrowUp}"
                            HorizontalAlignment="Center"
                            Width="6"
                            Height="4"/>
                </StackPanel>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="DockPosition" Value="Left">
                <Setter TargetName="PART_LeftIndicator" Property="Background" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="PART_LeftIndicator" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="LeftPlacementIndicator" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="LeftHighlightVisual" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="DockPosition" Value="Top">
                <Setter TargetName="PART_TopIndicator" Property="Background" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="PART_TopIndicator" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="TopPlacementIndicator" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="TopHighlightVisual" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="DockPosition" Value="Right">
                <Setter TargetName="PART_RightIndicator" Property="Background" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="PART_RightIndicator" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="RightPlacementIndicator" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="RightHighlightVisual" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="DockPosition" Value="Bottom">
                <Setter TargetName="PART_BottomIndicator" Property="Background" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="PART_BottomIndicator" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter TargetName="BottomPlacementIndicator" Property="Fill" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="BottomHighlightVisual" Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsLeftIndicatorVisible" Value="False">
                <Setter TargetName="PART_LeftIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsTopIndicatorVisible" Value="False">
                <Setter TargetName="PART_TopIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsRightIndicatorVisible" Value="False">
                <Setter TargetName="PART_RightIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="IsBottomIndicatorVisible" Value="False">
                <Setter TargetName="PART_BottomIndicator" Property="Visibility" Value="Collapsed"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RootCompassStyle" TargetType="dock:RootCompass">
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Panel.ZIndex" Value="100"/>
        <Setter Property="Template" Value="{StaticResource RootCompassControlTemplate}"/>
    </Style>
    <ControlTemplate x:Key="VisualCueControlTemplate" TargetType="dock:VisualCue">
        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
    </ControlTemplate>
    <Style x:Key="VisualCueStyle" TargetType="dock:VisualCue">
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="Opacity" Value="0.4"/>
        <Setter Property="Template" Value="{StaticResource VisualCueControlTemplate}"/>
    </Style>
    <Style x:Key="DocumentHostStyle" TargetType="dock:DocumentHost">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="MinWidth" Value="25"/>
        <Setter Property="MinHeight" Value="25"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="AllowDrop" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dock:DocumentHost">
                    <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Padding="{TemplateBinding Padding}">
                        <ContentPresenter
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                Content="{TemplateBinding Content}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RadDockingControlTemplate" TargetType="local:RadDocking">
        <Grid x:Name="Root">
            <Border x:Name="Border"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Padding="{TemplateBinding Padding}">
                <telerik:RadDockPanel x:Name="RootDockPanel">
                    <dock:AutoHideArea x:Name="LeftAutoHide" telerik:RadDockPanel.Dock="Left" TabStripPlacement="Left" BackgroundVisibility="Collapsed" TabOrientation="Vertical"/>
                    <dock:AutoHideArea x:Name="RightAutoHide" telerik:RadDockPanel.Dock="Right" TabStripPlacement="Right" BackgroundVisibility="Collapsed" TabOrientation="Vertical"/>
                    <dock:AutoHideArea x:Name="TopAutoHide" telerik:RadDockPanel.Dock="Top" TabStripPlacement="Top" BackgroundVisibility="Collapsed" TabOrientation="Horizontal"/>
                    <dock:AutoHideArea x:Name="BottomAutoHide" telerik:RadDockPanel.Dock="Bottom" TabStripPlacement="Bottom" BackgroundVisibility="Collapsed" TabOrientation="Horizontal"/>
                    <local:DockingPanel x:Name="ContentDockPanel" Margin="1">
                        <dock:DocumentHost x:Name="DocumentHost"/>
                    </local:DockingPanel>
                </telerik:RadDockPanel>
            </Border>
        </Grid>
    </ControlTemplate>
    <telerikAnimation:AnimationSelector x:Key="RadDockingAnimationSelector">
        <telerikAnimation:AnimationGroup AnimationName="Move">
            <telerikAnimation:MoveAnimation/>
            <telerikAnimation:ResizeAnimation/>
            <telerikAnimation:FadeAnimation Direction="In"/>
            <telerikAnimation:ScaleAnimation Direction="In" MinScale="0.7" MaxScale="1.0"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="Show">
            <telerikAnimation:FadeAnimation Direction="In"/>
            <telerikAnimation:ScaleAnimation Direction="In" MinScale="0.95" MaxScale="1.0"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="Hide" SpeedRatio="0.7">
            <telerikAnimation:FadeAnimation Direction="Out"/>
            <telerikAnimation:ScaleAnimation Direction="Out" MinScale="0.7" MaxScale="1.0"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="LeftIn">
            <telerikAnimation:SlideAnimation SlideMode="Top" Duration="{StaticResource AutoHideInAnimationDuration}" TargetElementName="PopupChild" Orientation="Horizontal" Direction="In"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="LeftOut">
            <telerikAnimation:SlideAnimation TargetElementName="PopupChild" SlideMode="Top" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Horizontal" Direction="Out"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="TopIn">
            <telerikAnimation:SlideAnimation Orientation="Vertical" Duration="{StaticResource AutoHideInAnimationDuration}" TargetElementName="PopupChild" Direction="In"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="TopOut">
            <telerikAnimation:SlideAnimation TargetElementName="PopupChild" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Vertical" Direction="Out"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="RightIn">
            <telerikAnimation:SlideAnimation SlideMode="Bottom" TargetElementName="PopupChild" Duration="{StaticResource AutoHideInAnimationDuration}" Orientation="Horizontal" Direction="In"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="RightOut">
            <telerikAnimation:SlideAnimation TargetElementName="PopupChild" SlideMode="Bottom" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Horizontal" Direction="Out"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="BottomIn">
            <telerikAnimation:SlideAnimation SlideMode="Bottom" Duration="{StaticResource AutoHideInAnimationDuration}" Orientation="Vertical" TargetElementName="PopupChild" Direction="In"/>
        </telerikAnimation:AnimationGroup>
        <telerikAnimation:AnimationGroup AnimationName="BottomOut">
            <telerikAnimation:SlideAnimation Duration="{StaticResource AutoHideOutAnimationDuration}" TargetElementName="PopupChild" Direction="Out" Orientation="Vertical" SlideMode="Bottom"/>
        </telerikAnimation:AnimationGroup>
    </telerikAnimation:AnimationSelector>
    <Style x:Key="RadDockingStyle" TargetType="local:RadDocking">
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=HighBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="1"/>
        <Setter Property="VisualCueStyle" Value="{StaticResource VisualCueStyle}"/>
        <Setter Property="Template" Value="{StaticResource RadDockingControlTemplate}"/>
        <Setter Property="CompassStyle" Value="{StaticResource CompassStyle}"/>
        <Setter Property="RootCompassStyle" Value="{StaticResource RootCompassStyle}"/>
        <Setter Property="telerikAnimation:AnimationManager.AnimationSelector" Value="{StaticResource RadDockingAnimationSelector}"/>
    </Style>
    <Style TargetType="dock:AutoHideArea" BasedOn="{StaticResource AutoHideAreaStyle}"/>
    <Style TargetType="local:RadPane" BasedOn="{StaticResource RadPaneStyle}"/>
    <Style TargetType="local:RadDocumentPane" BasedOn="{StaticResource RadPaneStyle}"/>
    <Style TargetType="dock:RadGridResizer" BasedOn="{StaticResource RadGridResizerStyle}"/>
    <Style TargetType="local:RadSplitContainer" BasedOn="{StaticResource RadSplitContainerStyle}"/>
    <Style TargetType="dock:PaneHeader" BasedOn="{StaticResource PaneHeaderStyle}"/>
    <Style TargetType="local:RadPaneGroup" BasedOn="{StaticResource RadPaneGroupStyle}"/>
    <Style TargetType="dock:ToolWindow" BasedOn="{StaticResource ToolWindowStyle}"/>
    <Style TargetType="dock:Compass" BasedOn="{StaticResource CompassStyle}"/>
    <Style TargetType="dock:RootCompass" BasedOn="{StaticResource RootCompassStyle}"/>
    <Style TargetType="dock:VisualCue" BasedOn="{StaticResource VisualCueStyle}"/>
    <Style TargetType="dock:DocumentHost" BasedOn="{StaticResource DocumentHostStyle}"/>
    <Style TargetType="local:RadDocking" BasedOn="{StaticResource RadDockingStyle}"/>
</ResourceDictionary>

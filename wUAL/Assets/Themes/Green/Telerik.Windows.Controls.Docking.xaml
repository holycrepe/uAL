                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Top</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="DockResizer" Property="VerticalAlignment" Value="Top"/>
                            <Setter TargetName="DockResizer" Property="HorizontalAlignment" Value="Stretch"/>
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeNS"/>
                            <Setter TargetName="DockResizer" Property="Placement">
                                <Setter.Value>
                                    <Dock>Top</Dock>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="DockResizer" Property="Height" Value="6"/>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0 6 0 0"/>
                        </Trigger>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Right</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="DockResizer" Property="Width" Value="6"/>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0 0 6 0"/>

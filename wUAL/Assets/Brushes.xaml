<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Properties="clr-namespace:wUAL.Properties;assembly=wUAL">
    <LinearGradientBrush x:Key="wUAL_ProgressBarForeground" StartPoint="0,0.5" EndPoint="1,0.5">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#FFF06414" />
            <GradientStop Offset="1" Color="#FF789FC3" />
            <GradientStop Offset="0.8" Color="#FFEAA61E" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="wUAL_LabelHeaderForeground" Color="#FF82B8EE" />
    <SolidColorBrush x:Key="wUAL_ProgressLabelTitle" Color="#FF004080" />
    <SolidColorBrush x:Key="wUAL_ProgressLabelCounts" Color="#FF004080" />
    <SolidColorBrush x:Key="wUAL_ProgressLabelText" Color="#FF0060BF" />
    <SolidColorBrush x:Key="wUAL_ContentDarkBackground" Color="#FF2C2828" />
    <SolidColorBrush x:Key="wUAL_ContentBackgroundDark" Color="#FF2C2828" />
</ResourceDictionary>